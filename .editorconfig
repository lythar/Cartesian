# editorconfig.org

root = true

# All files
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

# C# files
[*.cs]
indent_size = 4
indent_style = space

# Web files
[*.{ts,tsx,svelte,json,html,css,scss,yml,yaml}]
indent_size = 2
indent_style = space

# Markdown
[*.md]
trim_trailing_whitespace = false
